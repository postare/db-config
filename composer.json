{
    "name": "postare/db-settings",
    "description": "This is my package db-settings",
    "keywords": [
        "postare",
        "laravel",
        "db-settings"
    ],
    "homepage": "https://github.com/postare/db-settings",
    "support": {
        "issues": "https://github.com/postare/db-settings/issues",
        "source": "https://github.com/postare/db-settings"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Francesco",
            "email": "inerba@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Postare\\DbSettings\\": "src/",
            "Postare\\DbSettings\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Postare\\DbSettings\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Postare\\DbSettings\\DbSettingsServiceProvider"
            ],
            "aliases": {
                "DbSettings": "Postare\\DbSettings\\Facades\\DbSettings"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}